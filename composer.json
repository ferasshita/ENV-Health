{
    "name": "ferasshita/env-health",
    "description": "A PHP CLI security auditor that scans project environment and returns a Security Health Score",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feras Shita"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "EnvHealth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnvHealth\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/env-health"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
